{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-console": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-cycle": 0,
    "import/no-named-as-default": 0,
    "no-throw-literal": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 1,
    "consistent-return": 0,
    "no-undef": 0,
    "default-case": 0,
    "class-methods-use-this": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "no-empty": 0,
    "dot-notation": 0,
    "radix": 0,
    "no-unused-expressions": 1,
    "import/prefer-default-export": 0,
    "no-nested-ternary": 0,
    "prefer-destructuring": 0,
    "camelcase": 0,
    "no-plusplus": 0,
    "no-new": 0,
    "no-return-await": 0,
    "no-await-in-loop": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "no-empty-pattern": 0,
    "guard-for-in": 0,
    "no-restricted-globals": 0,
    "no-bitwise": 0,
    "no-extra-boolean-cast": 0
  },
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [
        ".ts",
        ".tsx"
      ]
    },
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    }
  }
}
